{
    "overrides": [
        {
            "files": ["*.yaml", "*.json"],
            "options": {
                "tabWidth": 4,
                "signleQuote": false
            }
        },
        {
            "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
            "options": {
                "semi": true,
                "printWidth": 80,
                "tabWidth": 4,
                "bracketSpacing": false,
                "explicitTypes": "always",
                "useTabs": false
            }
        }
    ]
}
